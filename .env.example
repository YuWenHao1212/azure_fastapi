# Azure OpenAI Configuration
EMBEDDING_ENDPOINT=https://your-endpoint.cognitiveservices.azure.com/openai/deployments/text-embedding-3-large/embeddings?api-version=2024-02-01
LLM2_ENDPOINT=https://your-endpoint.cognitiveservices.azure.com
LLM2_API_KEY=your-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# API Configuration
API_VERSION=v1
